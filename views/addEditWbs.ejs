                        abc = 0;
                        document.getElementById('wbsname').value = "";
                        document.getElementById('wbscode').value = "";
                        $('#wbsname').focus();
                        return false;
                    } else {
                        abc++;
                    }

                    <% } } %>

                    if (abc != 0) {
                        myFunction();
                        return true;
                    }
                }

            }

                

