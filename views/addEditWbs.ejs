            function allWbs() {
              resetError();
              var errorWbsname = $('#errorWbsname');
                var abc = 1;
                if ('<%=flag%>' == '0') {
                    <%if(allWbs.length>0){%>
                    <%  for(var j=0;j< allWbs.length; j++){%>
                    if ($('#wbsname').val() == '<%=allWbs[j].WBSName%>') {
