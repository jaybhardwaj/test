            function specialCharacter(evt) {
                var charCode = (evt.which) ? evt.which : event.keyCode
                if ((charCode != 38 && charCode > 32 && charCode < 46) || (charCode > 57 && charCode < 64) || charCode == 126 || charCode == 96) {
