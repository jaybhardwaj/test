    <script type="text/javascript">
   $(document).ready(function(){

    $.ajax({

        url: '/interviewerInfo',
        type: 'post',
        success: function(data) {
        console.log("Data for this interviewer: ",data);
var info=[];

    for(var i=0;i<data[0].length;i++){
var flag;
if(data[0][i].isDone==0){
flag=0;
}
else{
  flag=1;
}

      var mili=new Date(data[0][i].schedule);
      mili=mili.getTime();
      mili=JSON.stringify(mili);
      mili=mili.toString();
      info.push({"id": "293","url":"/interviewFeedback?id="+data[0][i].id+"&cid="+data[0][i].cid+"&flag="+flag,"title":data[0][i].name+" -- "+data[0][i].title+" -- "+data[0][i].stateName+" -Round-"+data[0][i].rounds,"class": data[0][i].isDone==0?"event-success":"","start":mili,"end":mili});
    }
         
          var calendar = $("#calendar").calendar(
            {  
            language: 'en-GB',
            tmpl_path: "/bower_components/bootstrap-calendar/tmpls/",
            events_source:  info
                    
            });
           }
            });  



            $('.btn-group button[data-calendar-nav]').each(function() {
              
    var $this = $(this);
    $this.click(function() {
        calendar.navigate($(this).data('calendar-nav'));
    });
});

$('.btn-group button[data-calendar-view]').each(function() {
    var $this = $(this);
    $this.click(function() {
        calendar.view($this.data('calendar-view'));
    });
});

$('#first_day').change(function(){
    var value = $(this).val();
    value = value.length ? parseInt(value) : null;
    calendar.setOptions({first_day: value});
    calendar.view();
});

$('#language').change(function(){
    calendar.setLanguage($(this).val());
    calendar.view();
});           
   });
        
