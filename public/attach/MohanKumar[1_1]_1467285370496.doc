                alert("Choose a password ! ");
                $('#1st').modal("show");
            } else {
                $('#hcname').val($('#mcname').val());
                $('#hemail').val($('#memail').val());
                $('#hpass').val($('#mpass').val());
                $("#1st").modal("hide");
                $("#2nd").modal("show");
            }
        }

        function model2() {
            var fsname = $('#mfname').val().trim();
            var lasname = $('#mlname').val().trim();
            var cont = $('#mcno').val().trim();
            if (fsname.length == '') {
                alert("Enter First Name ! ");
                $('#2nd').modal("show");
            } else if (lasname.length == '') {
                alert("Enter Last Name! ");
                $('#2nd').modal("show");
            } else if (cont.length == '' || cont.length < 10 || isNaN(cont)) {
                alert("Enter Valid Contact No.! ");
                $('#2nd').modal("show");
            } else {
                $('#hfname').val($('#mfname').val());
                $('#hlname').val($('#mlname').val());
                $('#hcno').val($('#mcno').val());
                $("#2nd").modal("hide");
                $("#3rd").modal("show");
            }
        }

        function model3() {

            $('#hmodules').val('');
