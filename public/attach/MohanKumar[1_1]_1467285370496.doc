            var eid = $('#memail').val().trim();
            var pas = $('#mpass').val().trim();

            //var validEmail= eid.substring(eid.indexOf('@')+1);
            var re1 = /^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;

            if (comp.length == '') {
